
@model List<EventHomeVM>

<div class="col-lg-12 col-md-12 col-sm-9 col-xs-12 fl-sm-none">
    <!-- Etkinlikler -->
    <div class="col-lg-12 col-md-12 col-sm-9 col-xs-12 sidebar-box box-white">
        <h3>@SiteLanguage.Activities</h3>
        <div class="showcase-box">
            @foreach (var item in Model)
            {
                <div class="showcase-logo">
                    <img src="@item.Logo" class="img-responsive">
                </div>
                <div class="showcase-caption">
                    <a href="#">@item.Title</a>
                </div>
                <div class="showcase-time">
                    @item.HuminizeDate
                </div>
            }
        </div>

        <div class="text-center">
            <a href="/events" class="show-all">@SiteLanguage.Show_All</a>
        </div>
    </div>
    <!-- Etkinlikler bitti -->
</div>
