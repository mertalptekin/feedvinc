
@using FeedVinc.WEB.UI.Models.ViewModels.Project.Profile
@model ProjectProfileVM

<div class="col-lg-12 col-md-12 col-sm-9 col-xs-12 mb20 fl-sm-none">
    <div class="box-white project-level">
        <h5>@SiteLanguage.Project_Level</h5>
        <div class="project-chart" data-percent="@Model.ProjectLevel" data-easyPieChart="">
            <span class="percent"></span>
        </div>


        @if (ViewBag.CurrentUserID == Model.ProjectOwnerID)
        {
            <div class="pda-row text-center">
                <a href="/project-task/@Model.ProjectSlugify/@Model.ProjectCode"> <h6>@SiteLanguage.GotoProjectTask</h6> </a>
            </div>
        }

        </div>
</div>
