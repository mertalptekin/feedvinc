@using FeedVinc.WEB.UI.Models.ViewModels.Project.Profile
@model List<ProjectProfilePhotoVM>

<div class="col-lg-12 col-md-12 col-sm-9 col-xs-12 fl-sm-none mb30">
    <div class="box-white project-images">
        <h5>Resimler</h5>
        @foreach (var item in Model)
        {
            <a href="/project-photo-edit/@item.ProjectSlugify/@item.ProjectCode">
                <img src="@item.PhotoPath" class="img-responsive">
            </a>
        }
    </div>
</div>
