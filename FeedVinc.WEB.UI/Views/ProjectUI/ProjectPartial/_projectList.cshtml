
@model IEnumerable<ProjectDetailVM>


@foreach (var item in Model)
{
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mb20">
        <div class="box-white project-box">
            <a href="/project-profile/@item.ProjectSlugify/@item.ProjectCode"><img src="@item.ProjectProfilePhoto" class="img-responsive" alt="@item.ProjectName" style="max-height:120px; max-width:120px;"></a> 
            <a href="/project-profile/@item.ProjectSlugify/@item.ProjectCode"><h2>@item.ProjectName</h2></a>
            <p>@item.SalesPitch</p>
            <div class="project-category">@item.ProjectCategoryName</div>
        </div>
    </div>
}


