
@model UserVM

<div  class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mb20 profile-about" style="display:none;" >
    <div class="box-white profile-about">
        <div class="p-col">
            <span>@SiteLanguage.User_BirthDate</span> @Model.BirthDate
        </div>
        <div class="p-col">
            <span>@SiteLanguage.Company_Info</span> @Model.Company
        </div>
        <div class="p-col">
            <span>@SiteLanguage.Phone_Number</span> @Model.PhoneNumber
        </div>
        <p>@Model.About</p>
    </div>
</div>
